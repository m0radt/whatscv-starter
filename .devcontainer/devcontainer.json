{
  "name": "whatscv-starter",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "app",
  "workspaceFolder": "/app",
  "shutdownAction": "stopCompose",
  "runServices": ["db", "app"],
  "overrideCommand": false,
  "remoteEnv": { "PYTHONPATH": "/app:/app/backend" },
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python", "ms-python.vscode-pylance"]
    }
  }
}
